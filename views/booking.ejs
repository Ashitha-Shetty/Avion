<!-- forms nedded, more like input ones-->

<%- include('partials/header'); %>
<div class="booking">
<form action="/saved" method="POST">
    <label for="username">Username:</label>
    <input type="text" name="username" value="<%= user %>" style="background-color: #284b564a; margin-left: 10px; width:200px;">
    <br>
    <br>
    <label for="email">Email:</label>
    <input type="text" name="email" value="<%= email %>" style="background-color: #284b564a; margin-left: 15px; width:230px;">
    <br>
    <br>
    <label for="address">Address:</label>
    <input type="text" name="address" value="<%= address %>" style="background-color: #284b564a; margin-left: 12px; width:210px;">
    <br>
    <br>
    <label for="phone">MobileNo:</label>
    <input type="text" name="phone" value="<%= phone %>" style="background-color: #284b564a; margin-left: 8px; width:200px;">
    <br>
    <br>
    <label for="from">From:</label>
    <input type="text" name="from" value="<%= from %>" style="background-color: #284b564a; margin-left: 15px; width:222px;">
    <br>
    <br>
    <label for="to">To:</label>
    <input type="text" name="to" value="<%= address %>" style="background-color: #284b564a; margin-left: 18px; width:240px;">
    <br>
    <br>
    <label for="date">Date:</label>
    <input type="text" name="date" style="background-color: #284b564a; margin-left: 15px; width:220px;">
    <br>
    <br>
    <button type="submit" class="btn btn-success" style="padding: 0px;">Confirm</button>
</form>
</div>

<%- include('partials/footer'); %>